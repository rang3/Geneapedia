USE `Wiki`;
DROP procedure IF EXISTS `Wiki`,`search`;

DELIMITER $$
USE `Wiki`$$
CREATE DEFINER=`root`@`localhost` PROCEDURE `search`(
    IN pname(45),
)
BEGIN
    SELECT Person.name, Child.child_name
    FROM Person
    INNER JOIN Child
    ON
    Person.name=Child.parent_name
END$$

DELIMITER ;
;
